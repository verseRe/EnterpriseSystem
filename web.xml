<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MultiKeywordSearch</display-name>
  <welcome-file-list>
    <welcome-file>balance.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>CheckBalanceServlet</servlet-name>
    <servlet-class>balance.CheckBalanceServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>PaymentServlet2</servlet-name>
    <servlet-class>payment.PaymentServlet2</servlet-class>
  </servlet>

<servlet-mapping>
    <servlet-name>CheckBalanceServlet</servlet-name>
    <url-pattern>/CheckBalanceServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>PaymentServlet2</servlet-name>
    <url-pattern>/payment</url-pattern>
</servlet-mapping>

</web-app>